{
  "debugMode": true,
  "bot": {
    "token": "",
    "activity": "/help",
    "statPostInterval": 30
  },
  "channels": {
    "command": "",
    "guild": "",
    "error": ""
  },
  "defaults": {
    "language": "en_US",
    "prefix": "at!"
  },
  "actions": {
    "warnKickCount": 3,
    "warnBanCount": 6
  },
  "apiKeys": {
    "KSOFT": ""
  },
  "botlists": [
    {
      "name": "topgg",
      "url": "https://top.gg/api/bots/$client_id/stats",
      "header": { "Authorization": "$token" },
      "body": { "server_count": "$guild_count" },
      "token": ""
    },
    {
      "name": "discords",
      "url": "https://discords.com/bots/api/bot/$client_id",
      "header": { "Authorization": "$token" },
      "body": { "server_count": "$guild_count" },
      "token": ""
    },
    {
      "name": "discordbotsgg",
      "url": "https://discord.bots.gg/api/v1/bots/$client_id/stats",
      "header": { "Authorization": "$token" },
      "body": { "guildCount": "$guild_count" },
      "token": ""
    },
    {
      "name": "discordboats",
      "url": "https://discord.boats/api/bot/$client_id",
      "header": { "Authorization": "$token" },
      "body": { "server_count": "$guild_count" },
      "token": ""
    },
    {
      "name": "discordbotlistcom",
      "url": "https://discordbotlist.com/api/v1/bots/$client_id/stats",
      "header": { "Authorization": "$token" },
      "body": { "guilds": "$guild_count" },
      "token": ""
    }
  ],
  "dbUrl": "mongodb://localhost:27017/AthenaV3"
}
